    context VolumeButton [ControlType="Button", AutomationId="SystemTrayIcon", ClassName="SystemTray.OmniButtonCenter"]:
    context PowerButton [ControlType="Button", AutomationId="SystemTrayIcon", ClassName="SystemTray.OmniButtonRight"]:
    context ClockButton [ControlType="Button", AutomationId="SystemTrayIcon", ClassName="SystemTray.OmniButton"]:
