    context SearchButton [ControlType="Button", Name="Search", AutomationId="SearchButton", ClassName="ToggleButton"]:
    context TaskViewButton [ControlType="Button", Name="Task View", AutomationId="TaskViewButton", ClassName="ToggleButton"]:
    context WidgetsButton [ControlType="Button", Name="Widgets", AutomationId="WidgetsButton", ClassName="ToggleButton"]:           
    context ChatButton [ControlType="Button", Name="Chat", AutomationId="MeetNowButton", ClassName="ToggleButton"]:   
